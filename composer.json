{
    "name": "stephanecoinon/turbine",
    "description": "Client for turbinehq.com API",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "St√©phane Coinon",
        "email": "me@stephanecoinon.co.uk"
    }],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "vlucas/phpdotenv": "^3.6",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "StephaneCoinon\\Turbine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StephaneCoinon\\Turbine\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StephaneCoinon\\Turbine\\Laravel\\TurbineServiceProvider"
            ]
        }
    }
}
